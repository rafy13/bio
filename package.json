{
  "name": "rabby-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio website for A K M Rabby",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--localstorage-file=./scratch/localstorage.json react-scripts start",
    "build": "cross-env NODE_OPTIONS=--localstorage-file=./scratch/localstorage.json react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

